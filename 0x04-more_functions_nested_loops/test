#include <stdio.h>

/*
*
*/

int main(void)
{
	long num = 18;
	long i;
	long j;
	/*int test = 25 / 2;

	printf("%d\n", test);*/

	for (i = num / 2; i > 0; i--)
	{
		for (j = 2; j < i / 2; j++)
		{
			if (i % j == 0)
		}

		if (num % i == 0)
		{
			printf("%ld ", i);
			break;
		}
	}
	printf("\n");

	return (0);
}
